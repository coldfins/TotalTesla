# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'TotalTesla' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TotalTesla
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
  pod 'TwitterKit'
  pod 'GoogleSignIn'
  pod 'Alamofire'
  pod 'PopupDialog'
  pod 'AFNetworking'
  pod 'ImageSlideshow'
  
  target 'TotalTeslaTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'TotalTeslaUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
